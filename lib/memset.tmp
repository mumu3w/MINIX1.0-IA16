        .code16
        .text



        .global memset
memset:
        mov %es, %dx
        mov %ds, %ax
        mov %ax, %es

        mov %sp, %bx
        push %di
        mov 2(%bx), %di
        mov 4(%bx), %ax
        mov 6(%bx), %cx
        cld
        rep
        stosb
        mov 2(%bx), %ax
        pop %di

        mov %dx, %es
        ret
